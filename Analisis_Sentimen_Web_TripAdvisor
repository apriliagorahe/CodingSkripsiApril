      "source": [
        "import nltk\n",
        "nltk.download('punkt')\n",
        "nltk.download('stopwords')\n",
        "nltk.download('wordnet')\n",
        "\n",
        "from nltk.corpus import stopwords\n",
        "from nltk.tokenize import word_tokenize\n",
        "from nltk.stem import WordNetLemmatizer\n",
        "\n",
        "def preprocess_text(text):\n",
        "    tokens = word_tokenize(text.lower())\n",
        "    tokens = [word for word in tokens if word.isalpha() and word not in stopwords.words('english')]\n",
        "    lemmatizer = WordNetLemmatizer()\n",
        "    tokens = [lemmatizer.lemmatize(word) for word in tokens]\n",
        "    return ' '.join(tokens)\n",
        "\n",
        "# Menggunakan nama kolom yang tepat 'Review Text'\n",
        "df['processed_text'] = df['Review Text'].apply(preprocess_text)\n",
        "\n",
        "# Menampilkan beberapa baris pertama untuk memastikan data telah diproses dengan benar\n",
        "df[['Review Text', 'processed_text']].head()\n"
      ]
    },
